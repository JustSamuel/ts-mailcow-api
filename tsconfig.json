{
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules",
    "dist",
    "src/test.ts"
  ],
  "typedocOptions": {
    "entryPoints": [
      "src/"
    ],
    "excludeNotDocumented": false,
    "excludeInternal": true,
    "excludeExternals": false,
    "excludeProtected": false,
    "out": "docs"
  },
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./src/",
    "typeRoots": [
      "./src/types.ts"
    ],
    "module": "CommonJS",
    "target": "es2015",
    "sourceMap": true,
    "allowJs": true,
    "declaration": true,
    "checkJs": false,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": false
  }
}
